{
    "build_osx_archs": {
        "macosx": [
            "arm64"
        ]
    },
    "build_params": {
        "base": [
            "--parallel",
            "--use_xcode",
            "--build_apple_framework",
            "--use_coreml",
            "--use_xnnpack",
            "--skip_tests",
            "--cmake_extra_defines=onnxruntime_BUILD_UNIT_TESTS=OFF"
        ],
        "macosx": [
            "--macos=MacOSX",
            "--apple_deploy_target=11.0"
        ]
    }
}
